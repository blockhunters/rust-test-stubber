{
	"name": "rust-test-stubber",
	"displayName": "Rust Test Generator",
	"description": "Generate test stubs with one click! This tool allows for automated generation of high quality unit test stubs in Rust langugage with simple command or click.",
	"version": "0.0.2",
	"publisher": "blockhunters",
	"keywords": ["rust", "unit test", "testing", "stubs", "generator", "test"],
	"repository": {
    "type": "git",
    "url": "https://github.com/blockhunters/rust-test-stubber.git"
	},
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:stubber.create"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "stubber.create",
				"title": "Rust Test Stubber: Create Stubs"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/tmp": "^0.2.0",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
